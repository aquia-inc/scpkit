{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Deny",
            "Action": [
                "sagemaker:CreateDomain"
            ],
            "Resource": "*",
            "Condition": {
                "StringEquals": {
                    "sagemaker:AppNetworkAccessType": "PublicInternetOnly"
                }
            }
        }
    ]
}