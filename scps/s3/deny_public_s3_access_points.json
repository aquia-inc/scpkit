{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "s3:CreateAccessPoint",
                "s3:PutAccessPointPolicy"
            ],
            "Resource": "arn:aws:s3:*:*:accesspoint/*",
            "Effect": "Deny",
            "Condition": {
                "StringNotEqualsIfExists": {
                    "s3:AccessPointNetworkOrigin": "vpc"
                }
            }
        }
    ]
}