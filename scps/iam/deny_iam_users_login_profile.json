{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "PreventIAMUserWithLoginprofile",
        "Effect": "Deny",
        "Action": [
            "iam:ChangePassword",
            "iam:CreateLoginProfile",
            "iam:UpdateAccountPasswordPolicy",
            "iam:UpdateLoginProfile"
        ],
        "Resource": ["*"]
      }
    ]
  }